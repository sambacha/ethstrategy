    "@actions/core": "^1.11.1",
    "@types/node": "^22",
    "chalk": "^5.4.1",
    "dotenv-cli": "^8.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.8.2",
    "viem": "^2.22.21"
  }
